<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/Arriba"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/blanco"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_blanco" />

                <ImageView
                    android:id="@+id/image1Back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/sierra" />

                <TextView
                    android:id="@+id/textImage1_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textImage1_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage1_1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textImage1_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage1_2"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <Button
                    android:id="@+id/buttonImg1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage1_3"
                    android:layout_marginLeft="30dp"
                    android:alpha="0"
                    android:onClick="intentClick"
                    android:text="RESERVAR" />

                <ImageView
                    android:id="@+id/image1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/sierra" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/blancoImg2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_blanco" />

                <ImageView
                    android:id="@+id/image2Back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/eme" />

                <TextView
                    android:id="@+id/textImage2_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textImage2_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage2_1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textImage2_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage2_2"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <Button
                    android:id="@+id/buttonImg2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage2_3"
                    android:layout_marginLeft="30dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:onClick="intentClick"
                    android:text="Button" />

                <ImageView
                    android:id="@+id/image2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/eme" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/Medio"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_toEndOf="@id/Arriba"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/blancoImg3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/alrgado_verticalcentro_blanco" />

                <ImageView
                    android:id="@+id/image3Back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_vertical" />

                <TextView
                    android:id="@+id/textImage3_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="60dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textImage3_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage3_1"
                    android:layout_marginLeft="25dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textImage3_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage3_2"
                    android:layout_marginLeft="25dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <Button
                    android:id="@+id/buttonImg3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage3_3"
                    android:layout_marginLeft="35dp"
                    android:alpha="0"
                    android:onClick="intentClick"
                    android:text="Button" />

                <ImageView
                    android:id="@+id/image3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_vertical" />
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/Abajo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_toEndOf="@id/Medio"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_blanco" />

                <ImageView
                    android:id="@+id/blancoImg4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cubo_cuadrado" />

                <TextView
                    android:id="@+id/textImage4_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textImage4_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage4_1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textImage4_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage4_2"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <Button
                    android:id="@+id/buttonImg4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage4_3"
                    android:layout_marginLeft="30dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:onClick="intentClick"
                    android:text="Button" />

                <ImageView
                    android:id="@+id/image4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cubo_cuadrado" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/cuadrado_blanco" />

                <ImageView
                    android:id="@+id/image5Back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="0"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/redonda_cuadrada" />

                <TextView
                    android:id="@+id/textImage5_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:text="TextView"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:alpha="0" />

                <TextView
                    android:id="@+id/textImage5_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage5_1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textImage5_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage5_2"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:text="TextView" />

                <Button
                    android:id="@+id/buttonImg5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textImage5_3"
                    android:layout_marginLeft="30dp"
                    android:layout_marginBottom="10dp"
                    android:alpha="0"
                    android:onClick="intentClick"
                    android:text="Button" />

                <ImageView
                    android:id="@+id/image5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/redonda_cuadrada" />
            </RelativeLayout>
        </LinearLayout>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>